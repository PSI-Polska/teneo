<?xml version="1.0" encoding="utf-8"?>
<persistence-mapping xmlns="http://www.elver.org/teneo/persistence"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

	<epackage namespace-uri="http://www.eclipse.org/emf/teneo/samples/emf/annotations/associationoverride">

		<eclass name="Person">
			<mapped-superclass />
			<property name="id">
				<id />
			</property>
			<property name="address">
				<many-to-one />
			</property>
		</eclass>
		
		<eclass name="Employee">
			<association-override>
				<name>address</name>
				<join-column>
					<name>employee_address_id</name>
				</join-column>
			</association-override>
		</eclass>
		
	</epackage>

</persistence-mapping>