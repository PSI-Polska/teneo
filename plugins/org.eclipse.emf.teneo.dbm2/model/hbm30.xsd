<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" 
	xmlns="http://hibernate.sourceforge.net/hbm30"
	xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" 
	targetNamespace="http://hibernate.sourceforge.net/hbm30">
	
		<!-- COMMON SIMPLE TYPE DEF -->
	<xs:simpleType name="fetch_join_select">
		<xs:restriction base="xs:string">
			<xs:enumeration value="join" />
			<xs:enumeration value="select" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="fetch">
		<xs:restriction base="xs:string">
			<xs:enumeration value="join" />
			<xs:enumeration value="select" />
			<xs:enumeration value="subselect" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="outerjoin">
		<xs:restriction base="xs:string">
			<xs:enumeration value="true" />
			<xs:enumeration value="false" />
			<xs:enumeration value="auto" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="lazy">
		<xs:restriction base="xs:string">
			<xs:enumeration value="false" />
			<xs:enumeration value="proxy" />
			<xs:enumeration value="no-proxy" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="lazy_false_proxy">
		<xs:restriction base="xs:string">
			<xs:enumeration value="false" />
			<xs:enumeration value="proxy" />
			<xs:enumeration value="no-proxy" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="lazy_true_false_extra">
		<xs:restriction base="xs:string">
			<xs:enumeration value="true" />
			<xs:enumeration value="false" />
			<xs:enumeration value="extra" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="generated">
		<xs:restriction base="xs:string">
			<xs:enumeration value="never" />
			<xs:enumeration value="insert" />
			<xs:enumeration value="always" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="generated_never_always">
		<xs:restriction base="xs:string">
			<xs:enumeration value="never" />
			<xs:enumeration value="always" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="cachemode">
		<xs:restriction base="xs:string">
			<xs:enumeration value="get" />
			<xs:enumeration value="ignore" />
			<xs:enumeration value="normal" />
			<xs:enumeration value="put" />
			<xs:enumeration value="refresh" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="lockmode">
		<xs:restriction base="xs:string">
			<xs:enumeration value="none" />
			<xs:enumeration value="read" />
			<xs:enumeration value="upgrade" />
			<xs:enumeration value="upgrade-nowait" />
			<xs:enumeration value="write" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="flushmode">
		<xs:restriction base="xs:string">
			<xs:enumeration value="auto" />
			<xs:enumeration value="never" />
			<xs:enumeration value="always" />
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="notfound">
		<xs:restriction base="xs:string">
			<xs:enumeration value="exception" />
			<xs:enumeration value="ignore" />
		</xs:restriction>
	</xs:simpleType>
	
	
	<!--  HIBERNATE-MAPPING -->
	<xs:complexType name="hibernate-mapping">
		<xs:sequence>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="meta"
				type="meta" />
			<xs:element maxOccurs="unbounded" minOccurs="0" name="typedef"
				type="typedef" />
			<xs:element maxOccurs="unbounded" minOccurs="0" name="import"
				type="import" />
			<xs:choice maxOccurs="unbounded" minOccurs="0">
				<xs:element name="class" type="class" />
				<xs:element name="subclass" type="subclass" />
				<xs:element name="joined-subclass" type="joined-subclass" />
				<xs:element name="union-subclass" type="union-subclass" />
			</xs:choice>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="resultset"
				type="resultset" />
			<xs:choice maxOccurs="unbounded" minOccurs="0">
				<xs:element name="query" type="query" />
				<xs:element name="sql-query" type="sql-query" />
			</xs:choice>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="filter-def"
				type="filter-def" />
			<xs:element maxOccurs="unbounded" minOccurs="0"
				name="database-object" type="database-object" />
		</xs:sequence>
		<xs:attribute name="schema" type="xs:string" />
		<xs:attribute name="catalog" type="xs:string" />
		<xs:attribute default="none" name="default-cascade" type="xs:string" />
		<xs:attribute default="property" name="default-access"
			type="xs:string" />
		<xs:attribute default="true" name="default-lazy" type="xs:boolean" />
		<xs:attribute default="true" name="auto-import" type="xs:boolean" />
		<xs:attribute name="package" type="xs:string" />
	</xs:complexType>

	<!-- META -->
	<xs:complexType name="meta">
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attribute name="attribute" type="xs:string" use="required" />
				<xs:attribute default="true" name="inherit" type="xs:boolean" />
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>

	<!--  TYPEDEF -->
	<xs:complexType name="typedef">
		<xs:sequence>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="param"
				type="param" />
		</xs:sequence>
		<xs:attribute name="class" type="xs:string" use="required" />
		<xs:attribute name="name" type="xs:string" use="required" />
	</xs:complexType>
	<xs:complexType name="import">
		<xs:attribute name="class" type="xs:string" use="required" />
		<xs:attribute name="rename" type="xs:string" />
	</xs:complexType>

	<!--  CLASS -->
	<xs:complexType name="class">
		<xs:sequence>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="meta"
				type="meta" />
			<xs:element minOccurs="0" name="subselect" type="xs:string" />
			<xs:element minOccurs="0" name="cache" type="cache" />
			<xs:element maxOccurs="unbounded" minOccurs="0" name="synchronize"
				type="synchronize" />
			<xs:element minOccurs="0" name="comment" type="xs:string" />
			<xs:element maxOccurs="unbounded" minOccurs="0" name="tuplizer"
				type="tuplizer" />
			<xs:choice>
				<xs:element name="id" type="id" />
				<xs:element name="composite-id" type="composite-id" />
			</xs:choice>
			<xs:element minOccurs="0" name="discriminator" type="discriminator" />
			<xs:element minOccurs="0" name="natural-id" type="natural-id" />
			<xs:choice minOccurs="0">
				<xs:element name="version" type="version" />
				<xs:element name="timestamp" type="timestamp" />
			</xs:choice>
			<xs:choice maxOccurs="unbounded" minOccurs="0">
				<xs:element name="property" type="property" />
				<xs:element name="many-to-one" type="many-to-one" />
				<xs:element name="one-to-one" type="one-to-one" />
				<xs:element name="component" type="component" />
				<xs:element name="dynamic-component" type="dynamic-component" />
				<xs:element name="properties" type="properties" />
				<xs:element name="any" type="any" />
				<xs:element name="map" type="map" />
				<xs:element name="set" type="set" />
				<xs:element name="list" type="list" />
				<xs:element name="bag" type="bag" />
				<xs:element name="idbag" type="idbag" />
				<xs:element name="array" type="array" />
				<xs:element name="primitive-array" type="primitive-array" />
			</xs:choice>
			<xs:choice>
				<xs:sequence>
					<xs:element maxOccurs="unbounded" minOccurs="0" name="join"
						type="join" />
					<xs:element maxOccurs="unbounded" minOccurs="0" name="subclass"
						type="subclass" />
				</xs:sequence>
				<xs:element maxOccurs="unbounded" minOccurs="0"
					name="joined-subclass" type="joined-subclass" />
				<xs:element maxOccurs="unbounded" minOccurs="0"
					name="union-subclass" type="union-subclass" />
			</xs:choice>
			<xs:element minOccurs="0" name="loader" type="loader" />
			<xs:element minOccurs="0" name="sql-insert" type="sql-insert" />
			<xs:element minOccurs="0" name="sql-update" type="sql-update" />
			<xs:element minOccurs="0" name="sql-delete" type="sql-delete" />
			<xs:element maxOccurs="unbounded" minOccurs="0" name="filter"
				type="filter" />
			<xs:element maxOccurs="unbounded" minOccurs="0" name="resultset"
				type="resultset" />
			<xs:choice maxOccurs="unbounded" minOccurs="0">
				<xs:element name="query" type="query" />
				<xs:element name="sql-query" type="sql-query" />
			</xs:choice>
		</xs:sequence>
		<xs:attribute name="entity-name" type="xs:string" />
		<xs:attribute name="name" type="xs:string" />
		<xs:attribute name="proxy" type="xs:string" />
		<xs:attribute name="lazy" type="xs:boolean" />
		<xs:attribute name="table" type="xs:string" />
		<xs:attribute name="schema" type="xs:string" />
		<xs:attribute name="catalog" type="xs:string" />
		<xs:attribute name="subselect" type="xs:string" />
		<xs:attribute name="discriminator-value" type="xs:string" />
		<xs:attribute default="true" name="mutable" type="xs:boolean" />
		<xs:attribute name="abstract" type="xs:boolean" />
		<xs:attribute default="implicit" name="polymorphism">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="implicit" />
					<xs:enumeration value="explicit" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="where" type="xs:string" />
		<xs:attribute name="persister" type="xs:string" />
		<xs:attribute default="false" name="dynamic-update" type="xs:boolean" />
		<xs:attribute default="false" name="dynamic-insert" type="xs:boolean" />
		<xs:attribute name="batch-size" type="xs:string" />
		<xs:attribute default="false" name="select-before-update"
			type="xs:boolean" />
		<xs:attribute default="version" name="optimistic-lock">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="none" />
					<xs:enumeration value="version" />
					<xs:enumeration value="dirty" />
					<xs:enumeration value="all" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="check" type="xs:string" />
		<xs:attribute name="rowid" type="xs:string" />
		<xs:attribute name="node" type="xs:string" />
	</xs:complexType>

	<!-- TUPLIZER -->
	<xs:complexType name="tuplizer">
		<xs:attribute name="entity-mode">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="pojo" />
					<xs:enumeration value="dom4j" />
					<xs:enumeration value="dynamic-map" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="class" type="xs:string" use="required" />
	</xs:complexType>

	<!--  FILTER-DEF -->
	<xs:complexType mixed="true" name="filter-def">
		<xs:choice maxOccurs="unbounded" minOccurs="0">
			<xs:element name="filter-param" type="filter-param" />
		</xs:choice>
		<xs:attribute name="name" type="xs:string" use="required" />
		<xs:attribute name="condition" type="xs:string" />
	</xs:complexType>
	<xs:complexType name="filter-param">
		<xs:attribute name="name" type="xs:string" use="required" />
		<xs:attribute name="type" type="xs:string" use="required" />
	</xs:complexType>

	<!-- FILTER -->
	<xs:complexType name="filter">
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attribute name="name" type="xs:string" use="required" />
				<xs:attribute name="condition" type="xs:string" />
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>

	<!-- JOIN -->
	<xs:complexType name="join">
		<xs:sequence>
			<xs:element minOccurs="0" name="subselect" type="xs:string" />
			<xs:element minOccurs="0" name="comment" type="xs:string" />
			<xs:element name="key" type="key" />
			<xs:choice maxOccurs="unbounded" minOccurs="0">
				<xs:element name="property" type="property" />
				<xs:element name="many-to-one" type="many-to-one" />
				<xs:element name="component" type="component" />
				<xs:element name="dynamic-component" type="dynamic-component" />
				<xs:element name="any" type="any" />
			</xs:choice>
			<xs:element minOccurs="0" name="sql-insert" type="sql-insert" />
			<xs:element minOccurs="0" name="sql-update" type="sql-update" />
			<xs:element minOccurs="0" name="sql-delete" type="sql-delete" />
		</xs:sequence>
		<xs:attribute name="table" type="xs:string" use="required" />
		<xs:attribute name="schema" type="xs:string" />
		<xs:attribute name="catalog" type="xs:string" />
		<xs:attribute name="subselect" type="xs:string" />
		<xs:attribute default="join" name="fetch" type="fetch" />
		<xs:attribute default="false" name="inverse" type="xs:boolean" />
		<xs:attribute default="false" name="optional" type="xs:boolean" />
	</xs:complexType>

	<!-- NATURAL-ID -->
	<xs:complexType name="natural-id">
		<xs:choice maxOccurs="unbounded" minOccurs="0">
			<xs:element name="property" type="property" />
			<xs:element name="many-to-one" type="many-to-one" />
			<xs:element name="component" type="component" />
			<xs:element name="dynamic-component" type="dynamic-component" />
			<xs:element name="any" type="any" />
		</xs:choice>
		<xs:attribute default="false" name="mutable" type="xs:boolean" />
	</xs:complexType>

	<!--  ID -->
	<xs:complexType name="id">
		<xs:sequence>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="meta"
				type="meta" />
			<xs:element maxOccurs="unbounded" minOccurs="0" name="column"
				type="column" />
			<xs:element minOccurs="0" name="type" type="type" />
			<xs:element minOccurs="0" name="generator" type="generator" />
		</xs:sequence>
		<xs:attribute name="name" type="xs:string" />
		<xs:attribute name="node" type="xs:string" />
		<xs:attribute name="access" type="xs:string" />
		<xs:attribute name="column" type="xs:string" />
		<xs:attribute name="type" type="xs:string" />
		<xs:attribute name="length" type="xs:string" />
		<xs:attribute name="unsaved-value" type="xs:string" />
	</xs:complexType>

	<!--  COMPOSITE-ID -->
	<xs:complexType name="composite-id">
		<xs:sequence>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="meta"
				type="meta" />
			<xs:choice maxOccurs="unbounded">
				<xs:element name="key-property" type="key-property" />
				<xs:element name="key-many-to-one" type="key-many-to-one" />
			</xs:choice>
		</xs:sequence>
		<xs:attribute name="class" type="xs:string" />
		<xs:attribute default="false" name="mapped" type="xs:boolean" />
		<xs:attribute name="name" type="xs:string" />
		<xs:attribute name="node" type="xs:string" />
		<xs:attribute name="access" type="xs:string" />
		<xs:attribute default="undefined" name="unsaved-value">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="undefined" />
					<xs:enumeration value="any" />
					<xs:enumeration value="none" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>

	<!-- DISCRIMINATOR -->
	<xs:complexType name="discriminator">
		<xs:choice minOccurs="0">
			<xs:element name="column" type="column" />
			<xs:element name="formula" type="xs:string" />
		</xs:choice>
		<xs:attribute name="column" type="xs:string" />
		<xs:attribute name="formula" type="xs:string" />
		<xs:attribute default="string" name="type" type="xs:string" />
		<xs:attribute default="true" name="not-null" type="xs:boolean" />
		<xs:attribute name="length" type="xs:string" />
		<xs:attribute default="false" name="force" type="xs:boolean" />
		<xs:attribute default="true" name="insert" type="xs:boolean" />
	</xs:complexType>

	<!--  VERSION -->
	<xs:complexType name="version">
		<xs:sequence>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="meta"
				type="meta" />
			<xs:element maxOccurs="unbounded" minOccurs="0" name="column"
				type="column" />
		</xs:sequence>
		<xs:attribute name="name" type="xs:string" use="required" />
		<xs:attribute name="node" type="xs:string" />
		<xs:attribute name="access" type="xs:string" />
		<xs:attribute name="column" type="xs:string" />
		<xs:attribute default="integer" name="type" type="xs:string" />
		<xs:attribute default="undefined" name="unsaved-value">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="null" />
					<xs:enumeration value="negative" />
					<xs:enumeration value="undefined" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute default="never" name="generated"
			type="generated_never_always" />
		<xs:attribute name="insert" type="xs:boolean" />
	</xs:complexType>

	<!-- TIMESTAMP -->
	<xs:complexType name="timestamp">
		<xs:sequence>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="meta"
				type="meta" />
		</xs:sequence>
		<xs:attribute name="name" type="xs:string" use="required" />
		<xs:attribute name="node" type="xs:string" />
		<xs:attribute name="column" type="xs:string" />
		<xs:attribute name="access" type="xs:string" />
		<xs:attribute default="null" name="unsaved-value">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="null" />
					<xs:enumeration value="undefined" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>

		<xs:attribute default="vm" name="source">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="vm" />
					<xs:enumeration value="db" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute default="never" name="generated"
			type="generated_never_always" />
	</xs:complexType>

	<!--  SUBCLASS -->
	<xs:complexType name="subclass">
		<xs:sequence>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="meta"
				type="meta" />
			<xs:element maxOccurs="unbounded" minOccurs="0" name="tuplizer"
				type="tuplizer" />
			<xs:element maxOccurs="unbounded" minOccurs="0" name="synchronize"
				type="synchronize" />
			<xs:choice maxOccurs="unbounded" minOccurs="0">
				<xs:element name="property" type="property" />
				<xs:element name="many-to-one" type="many-to-one" />
				<xs:element name="one-to-one" type="one-to-one" />
				<xs:element name="component" type="component" />
				<xs:element name="dynamic-component" type="dynamic-component" />
				<xs:element name="any" type="any" />
				<xs:element name="map" type="map" />
				<xs:element name="set" type="set" />
				<xs:element name="list" type="list" />
				<xs:element name="bag" type="bag" />
				<xs:element name="idbag" type="idbag" />
				<xs:element name="array" type="array" />
				<xs:element name="primitive-array" type="primitive-array" />
			</xs:choice>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="join"
				type="join" />
			<xs:element maxOccurs="unbounded" minOccurs="0" name="subclass"
				type="subclass" />
			<xs:element minOccurs="0" name="loader" type="loader" />
			<xs:element minOccurs="0" name="sql-insert" type="sql-insert" />
			<xs:element minOccurs="0" name="sql-update" type="sql-update" />
			<xs:element minOccurs="0" name="sql-delete" type="sql-delete" />
			<xs:element maxOccurs="unbounded" minOccurs="0" name="resultset"
				type="resultset" />
			<xs:choice maxOccurs="unbounded" minOccurs="0">
				<xs:element name="query" type="query" />
				<xs:element name="sql-query" type="sql-query" />
			</xs:choice>
		</xs:sequence>
		<xs:attribute name="entity-name" type="xs:string" />
		<xs:attribute name="name" type="xs:string" />
		<xs:attribute name="proxy" type="xs:string" />
		<xs:attribute name="discriminator-value" type="xs:string" />
		<xs:attribute default="false" name="dynamic-update" type="xs:boolean" />
		<xs:attribute default="false" name="dynamic-insert" type="xs:boolean" />
		<xs:attribute default="false" name="select-before-update"
			type="xs:boolean" />
		<xs:attribute name="extends" type="xs:string" />
		<xs:attribute name="lazy" type="xs:boolean" />
		<xs:attribute name="abstract" type="xs:boolean" />
		<xs:attribute name="persister" type="xs:string" />
		<xs:attribute name="batch-size" type="xs:string" />
		<xs:attribute name="node" type="xs:string" />
	</xs:complexType>

	<!--  JOINED-SUBCLASS -->
	<xs:complexType name="joined-subclass">
		<xs:sequence>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="meta"
				type="meta" />
			<xs:element minOccurs="0" name="subselect" type="xs:string" />
			<xs:element maxOccurs="unbounded" minOccurs="0" name="synchronize"
				type="synchronize" />
			<xs:element minOccurs="0" name="comment" type="xs:string" />
			<xs:element maxOccurs="unbounded" minOccurs="0" name="tuplizer"
				type="tuplizer" />
			<xs:element name="key" type="key" />
			<xs:choice maxOccurs="unbounded" minOccurs="0">
				<xs:element name="property" type="property" />
				<xs:element name="many-to-one" type="many-to-one" />
				<xs:element name="one-to-one" type="one-to-one" />
				<xs:element name="component" type="component" />
				<xs:element name="dynamic-component" type="dynamic-component" />
				<xs:element name="properties" type="properties" />
				<xs:element name="any" type="any" />
				<xs:element name="map" type="map" />
				<xs:element name="set" type="set" />
				<xs:element name="list" type="list" />
				<xs:element name="bag" type="bag" />
