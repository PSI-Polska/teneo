			<xs:element name="key-many-to-one" type="key-many-to-one" />
		</xs:choice>
		<xs:attribute name="class" type="xs:string" use="required" />
	</xs:complexType>

	<!-- COMPOSITE-INDEX -->
	<xs:complexType name="composite-index">
		<xs:choice maxOccurs="unbounded">
			<xs:element name="key-property" type="key-property" />
			<xs:element name="key-many-to-one" type="key-many-to-one" />
		</xs:choice>
		<xs:attribute name="class" type="xs:string" use="required" />
	</xs:complexType>

	<!-- MANY-TO-ANY -->
	<xs:complexType name="many-to-any">
		<xs:sequence>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="meta-value"
				type="meta-value" />
			<xs:element name="column" type="column" />
			<xs:element maxOccurs="unbounded" name="column" type="column" />
		</xs:sequence>
		<xs:attribute name="id-type" type="xs:string" use="required" />
		<xs:attribute name="meta-type" type="xs:string" />
	</xs:complexType>

	<!-- INDEX-MANY-TO-MANY -->
	<xs:complexType name="index-many-to-any">
		<xs:sequence>
			<xs:element name="column" type="column" />
			<xs:element maxOccurs="unbounded" name="column" type="column" />
		</xs:sequence>
		<xs:attribute name="id-type" type="xs:string" use="required" />
		<xs:attribute name="meta-type" type="xs:string" />
	</xs:complexType>

	<!-- COLLECTION-ID -->
	<xs:complexType name="collection-id">
		<xs:sequence>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="meta"
				type="meta" />
			<xs:element maxOccurs="unbounded" minOccurs="0" name="column"
				type="column" />
			<xs:element name="generator" type="generator" />
		</xs:sequence>
		<xs:attribute name="column" type="xs:string" use="required" />
		<xs:attribute name="type" type="xs:string" use="required" />
		<xs:attribute name="length" type="xs:string" />
	</xs:complexType>

	<!-- GENERATOR -->
	<xs:complexType name="generator">
		<xs:sequence>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="param"
				type="param" />
		</xs:sequence>
		<xs:attribute name="class" type="xs:string" use="required" />
	</xs:complexType>

	<!-- PARAM -->
	<xs:complexType name="param">
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attribute name="name" type="xs:string" use="required" />
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>

	<!-- COLUMN -->
	<xs:complexType name="column">
		<xs:sequence>
			<xs:element minOccurs="0" name="comment" type="xs:string" />
		</xs:sequence>
		<xs:attribute name="name" type="xs:string" use="required" />
		<xs:attribute name="length" type="xs:string" />
		<xs:attribute name="precision" type="xs:string" />
		<xs:attribute name="scale" type="xs:string" />
		<xs:attribute name="not-null" type="xs:boolean" />
		<xs:attribute name="unique" type="xs:boolean" />
		<xs:attribute name="unique-key" type="xs:string" />
		<xs:attribute name="sql-type" type="xs:string" />
		<xs:attribute name="index" type="xs:string" />
		<xs:attribute name="check" type="xs:string" />
		<xs:attribute name="default" type="xs:string" />
	</xs:complexType>

	<!-- CACHE -->
	<xs:complexType name="cache">
		<xs:attribute name="usage" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="read-only" />
					<xs:enumeration value="read-write" />
					<xs:enumeration value="nonstrict-read-write" />
					<xs:enumeration value="transactional" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="region" type="xs:string" />
		<xs:attribute default="all" name="include">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="all" />
					<xs:enumeration value="non-lazy" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>

	<!-- LOADER -->
	<xs:complexType name="loader">
		<xs:attribute name="query-ref" type="xs:string" use="required" />
	</xs:complexType>

	<!-- QUERY -->
	<xs:complexType mixed="true" name="query">
		<xs:choice maxOccurs="unbounded" minOccurs="0">
			<xs:element name="query-param" type="query-param" />
		</xs:choice>
		<xs:attribute name="name" type="xs:string" use="required" />
		<xs:attribute name="flush-mode" type="flushmode" />
		<xs:attribute default="false" name="cacheable" type="xs:boolean" />
		<xs:attribute name="cache-region" type="xs:string" />
		<xs:attribute name="fetch-size" type="xs:string" />
		<xs:attribute name="timeout" type="xs:string" />
		<xs:attribute name="cache-mode" type="cachemode" />
		<xs:attribute name="read-only" type="xs:boolean" />
		<xs:attribute name="comment" type="xs:string" />
	</xs:complexType>

	<!-- SQL-QUERY -->
	<xs:complexType mixed="true" name="sql-query">
		<xs:choice maxOccurs="unbounded" minOccurs="0">
			<xs:element name="return-scalar" type="return-scalar" />
			<xs:element name="return" type="return" />
			<xs:element name="return-join" type="return-join" />
			<xs:element name="load-collection" type="load-collection" />
			<xs:element name="synchronize" type="synchronize" />
			<xs:element name="query-param" type="query-param" />
		</xs:choice>
		<xs:attribute name="name" type="xs:string" use="required" />
		<xs:attribute name="resultset-ref" type="xs:string" />
		<xs:attribute name="flush-mode" type="flushmode" />
		<xs:attribute default="false" name="cacheable" type="xs:boolean" />
		<xs:attribute name="cache-region" type="xs:string" />
		<xs:attribute name="fetch-size" type="xs:string" />
		<xs:attribute name="timeout" type="xs:string" />
		<xs:attribute name="cache-mode" type="cachemode" />
		<xs:attribute name="read-only" type="xs:boolean" />
		<xs:attribute name="comment" type="xs:string" />
		<xs:attribute default="false" name="callable" type="xs:boolean" />
	</xs:complexType>

	<!-- QUERY-PARAM -->
	<xs:complexType name="query-param">
		<xs:attribute name="name" type="xs:string" use="required" />
		<xs:attribute name="type" type="xs:string" use="required" />
	</xs:complexType>

	<!-- RESULTSET -->
	<xs:complexType name="resultset">
		<xs:choice maxOccurs="unbounded" minOccurs="0">
			<xs:element name="return-scalar" type="return-scalar" />
			<xs:element name="return" type="return" />
			<xs:element name="return-join" type="return-join" />
			<xs:element name="load-collection" type="load-collection" />
		</xs:choice>
		<xs:attribute name="name" type="xs:string" use="required" />
	</xs:complexType>

	<!-- RETURN -->
	<xs:complexType name="return">
		<xs:sequence maxOccurs="unbounded" minOccurs="0">
			<xs:element minOccurs="0" name="return-discriminator"
				type="return-discriminator" />
			<xs:element name="return-property" type="return-property" />
		</xs:sequence>
		<xs:attribute name="alias" type="xs:string" />
		<xs:attribute name="entity-name" type="xs:string" />
		<xs:attribute name="class" type="xs:string" />
		<xs:attribute default="read" name="lock-mode" type="lockmode" />
	</xs:complexType>

	<!-- RETURN-PROPERY -->
	<xs:complexType name="return-property">
		<xs:sequence>
			<xs:element maxOccurs="unbounded" minOccurs="0"
				name="return-column" type="return-column" />
		</xs:sequence>
		<xs:attribute name="name" type="xs:string" use="required" />
		<xs:attribute name="column" type="xs:string" />
	</xs:complexType>

	<!-- RETURN-COLUMN -->
	<xs:complexType name="return-column">
		<xs:attribute name="name" type="xs:string" use="required" />
	</xs:complexType>

	<!-- RETURN-DISCRIMINATOR -->
	<xs:complexType name="return-discriminator">
		<xs:attribute name="column" type="xs:string" use="required" />
	</xs:complexType>

	<!-- RETURN-JOIN -->
	<xs:complexType name="return-join">
		<xs:sequence maxOccurs="unbounded" minOccurs="0">
			<xs:element name="return-property" type="return-property" />
		</xs:sequence>
		<xs:attribute name="alias" type="xs:string" use="required" />
		<xs:attribute name="property" type="xs:string" use="required" />
		<xs:attribute default="read" name="lock-mode" type="lockmode" />
	</xs:complexType>

	<!-- LOAD-COLLECTION -->
	<xs:complexType name="load-collection">
		<xs:sequence maxOccurs="unbounded" minOccurs="0">
			<xs:element name="return-property" type="return-property" />
		</xs:sequence>
		<xs:attribute name="alias" type="xs:string" use="required" />
		<xs:attribute name="role" type="xs:string" use="required" />
		<xs:attribute default="read" name="lock-mode" type="lockmode" />
	</xs:complexType>

	<!-- RETURN-SCALAR -->
	<xs:complexType name="return-scalar">
		<xs:attribute name="column" type="xs:string" use="required" />
		<xs:attribute name="type" type="xs:string" />
	</xs:complexType>

	<!-- SYNCHRONIZE -->
	<xs:complexType name="synchronize">
		<xs:attribute name="table" type="xs:string" use="required" />
	</xs:complexType>

	<!-- SQL-INSERT -->
	<xs:complexType name="sql-insert">
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attribute default="false" name="callable" type="xs:boolean" />
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>

	<!-- SQL-UPDATE -->
	<xs:complexType name="sql-update">
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attribute default="false" name="callable" type="xs:boolean" />
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>

	<!-- SQL-DELETE -->
	<xs:complexType name="sql-delete">
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attribute default="false" name="callable" type="xs:boolean" />
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>

	<!-- SQL-DELETE-ALL -->
	<xs:complexType name="sql-delete-all">
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attribute default="false" name="callable" type="xs:boolean" />
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>

	<!-- DATABASE-OBJECT -->
	<xs:complexType name="database-object">
		<xs:sequence>
			<xs:choice>
				<xs:element name="definition" type="definition" />
				<xs:sequence>
					<xs:element name="create" type="xs:string" />
					<xs:element name="drop" type="xs:string" />
				</xs:sequence>
			</xs:choice>
			<xs:element maxOccurs="unbounded" minOccurs="0"
				name="dialect-scope" type="dialect-scope" />
		</xs:sequence>
	</xs:complexType>

	<!-- DEFINITION -->
	<xs:complexType name="definition">
		<xs:attribute name="class" type="xs:string" use="required" />
	</xs:complexType>

	<!-- DIALECT-SCOPE -->
	<xs:complexType name="dialect-scope">
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attribute name="name" type="xs:string" use="required" />
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
</xs:schema>
