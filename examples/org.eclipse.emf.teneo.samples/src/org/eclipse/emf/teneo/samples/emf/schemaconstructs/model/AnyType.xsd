<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
    targetNamespace="http://www.eclipse.org/emf/teneo/samples/emf/schemaconstructs/anytype"
	xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore"
    xmlns="http://www.eclipse.org/emf/teneo/samples/emf/schemaconstructs/anytype">
   
   	<!-- Tests:
   	- xsd:anyType type
   	- xsd:any
   	-->
   
   	<!-- base type -->
	<xs:complexType name="testAny">
  		<xs:sequence>
   			<xs:element name="name" type="xs:string"/> 
   			<xs:element name="singleAnyType" type="xs:anyType"/>
   			<xs:element name="multiAnyType" type="xs:anyType" maxOccurs="unbounded"/>
   			<xs:any ecore:name="myAny" namespace="##other"/>
   			<xs:any maxOccurs="unbounded" namespace="##other"/>
  		</xs:sequence>
 	</xs:complexType>
 	
 	<!-- Type to be used to set any above -->
 	<xs:complexType name="myA" ecore:name="A">
 		<xs:sequence>
 			<xs:element name="name" type="xs:string"/>
 			<xs:element name="doub" type="xs:double"/>
 			<xs:element name="lon" type="xs:long"/>
 			<xs:element name="myB" type="xs:IDREF" ecore:reference="B"/>
 		</xs:sequence>
 	</xs:complexType>

 	<xs:complexType name="B">
 		<xs:sequence>
 			<xs:element name="name" type="xs:string"/>
 		</xs:sequence>
 	</xs:complexType>
 </xs:schema>
